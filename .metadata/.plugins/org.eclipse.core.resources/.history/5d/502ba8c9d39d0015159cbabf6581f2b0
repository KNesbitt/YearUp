
public class StudentRoster 
{
	String m_students = null;
	String m_lcName = null;
	int m_lcID = 0;
	
	StudentRoster(String lcName, int lcID)
	{
		
	}

	public String getM_students() {
		return m_students;
	}

	public String getM_lcName() {
		return m_lcName;
	}

	public int getM_lcID() {
		return m_lcID;
	}

	private void setM_lcName(String m_lcName) {
		this.m_lcName = m_lcName;
	}

	private void setM_lcID(int m_lcID) {
		this.m_lcID = m_lcID;
	}
}

