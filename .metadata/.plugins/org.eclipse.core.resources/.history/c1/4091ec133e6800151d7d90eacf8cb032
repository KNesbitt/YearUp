
public class Circle {

		
	String m_color = null;
	double m_radius = 0;
	public static void main(String[] args) 
	{
		
	}

	
	Circle(int radius)
	{
			m_radius = radius;
			m_color = "grey";
	}
	Circle(int radius, String color)
	{
			m_radius = radius;
			m_color = color;
	}
	
	double CalculateArea()
	{
			double area = 3.14 * m_radius * m_radius;
			return area;
	}
	
	
}

